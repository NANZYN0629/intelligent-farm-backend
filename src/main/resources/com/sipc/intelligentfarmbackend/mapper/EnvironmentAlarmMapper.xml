<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sipc.intelligentfarmbackend.mapper.EnvironmentAlarmMapper">

<!--    获取所有环境报警设置-->
    <select id="getByFieldId" resultType="com.sipc.intelligentfarmbackend.pojo.EnvironmentAlarmSet">
        SELECT * FROM environment_alarm_set
        WHERE field_id = #{fieldId}
    </select>


</mapper>

