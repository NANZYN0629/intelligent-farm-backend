<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sipc.intelligentfarmbackend.mapper.ProductionForecastMapper">
    <select id="getByFieldId" resultType="com.sipc.intelligentfarmbackend.pojo.ProductionForecast">
        SELECT * FROM production_forecast
        WHERE field_id = #{fieldId}
